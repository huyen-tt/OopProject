<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ArrayListController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" />
      <ImageView fitHeight="400.0" fitWidth="600.0">
               <image>
                  <Image url="@images.jpg" />
               </image>
            </ImageView>
      <TextField fx:id="valueAdd" layoutX="37.0" layoutY="62.0" onAction="#addItem" prefHeight="25.0" prefWidth="87.0" />
      <TextField fx:id="indexInsert" layoutX="213.0" layoutY="62.0" prefHeight="25.0" prefWidth="87.0" />
      <TextField fx:id="valueInsert" layoutX="306.0" layoutY="62.0" onAction="#insertItem" prefHeight="25.0" prefWidth="87.0" />
      <TextField fx:id="indexDelete" layoutX="471.0" layoutY="62.0" onAction="#deleteItem" prefHeight="25.0" prefWidth="87.0" />
      <Button fx:id="add" layoutX="37.0" layoutY="99.0" mnemonicParsing="false" onAction="#addItem" prefHeight="25.0" prefWidth="87.0" style="-fx-background-color: lightblue;" text="Add Item" />
      <Button fx:id="insertItem" layoutX="264.0" layoutY="99.0" mnemonicParsing="false" onAction="#insertItem" text="Insert Item" />
      <Button fx:id="deleteItem" layoutX="471.0" layoutY="99.0" mnemonicParsing="false" onAction="#deleteItem" prefHeight="25.0" prefWidth="87.0" style="-fx-background-color: pink;" text="Delete Item" />
      <Label layoutX="66.0" layoutY="37.0" text="Value" />
      <Label layoutX="243.0" layoutY="37.0" text="Index" />
      <Label layoutX="336.0" layoutY="37.0" text="Value" />
      <Label layoutX="501.0" layoutY="29.0" text="Index" />
      <Button fx:id="back" layoutX="504.0" layoutY="341.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: pink;" text="Back" />
   </children>
</AnchorPane>
